<div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content" style="background:#fbfaf3">

      <%= form_with model: list, url: create_list_path, local: false do |f| %>
      <div class="modal-header" style="background:#F5DEB3;">
        <h5 class="modal-title" id="exampleModalCenterTitle">食材新規作成</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div class="modal-body">
        <%= render 'public/lists/list_form', f: f %>
      </div>
      <% end %>

    </div>
  </div>
</div>